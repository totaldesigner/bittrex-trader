{
  "name": "noin",
  "version": "1.0.0",
  "main": "server/index.js",
  "pre-commit": "lint:tslint",
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "description": "noin",
  "scripts": {
    "build": "npm run build:clean && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js -p --progress --colors",
    "build:development": "npm run build:clean && cross-env NODE_ENV=development webpack --config internals/webpack/webpack.dev.js --progress --colors",
    "build:clean": "node ./internals/scripts/clean.js",
    "lint": "npm run lint:tslint && npm run lint:css",
    "lint:css": "stylelint \"app/components/**/*.css\" \"app/containers/**/*.css\" \"app/global.css\"",
    "lint:tslint": "tslint \"app/**/*.tsx\" \"app/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development LOG_LEVEL=debug node server",
    "start:api": "node server/api.js",
    "start:local": "cross-env NODE_ENV=development LOCAL=true LOG_LEVEL=debug node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pretest": "npm run test:clean && npm run lint",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "webpack-defaults": "webpack-defaults"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "!app/**/*.test.{ts,tsx}",
      "!app/definitions.d.ts",
      "!app/global.d.ts",
      "!app/index.tsx",
      "!app/utils/request.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "testRegex": "tests/.*\\.test\\.(ts|tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cross-env": "^3.2.4",
    "dateformat": "^2.0.0",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "es6-collections": "^0.5.6",
    "es6-promise": "^4.1.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "intl": "^1.2.5",
    "ip": "^1.1.5",
    "location-origin": "^1.1.4",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "minimist": "^1.2.0",
    "node-bittrex-api": "^0.7.6",
    "object-assign-mdn": "^1.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-load-script": "^0.0.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.23.4",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-thunk": "^2.2.0",
    "redux-thunk-actions": "^1.1.6",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "ua-parser-js": "^0.7.12",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/classnames": "^0.0.32",
    "@types/jest": "^21.1.5",
    "@types/node": "^7.0.4",
    "@types/react": "^16.0.7",
    "@types/redux-actions": "^1.2.2",
    "@types/webpack": "^2.2.4",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.2",
    "basscss": "^8.0.3",
    "basscss-addons": "^1.0.0",
    "basscss-basic": "^1.0.0",
    "cheerio": "^0.22.0",
    "circular-dependency-plugin": "^2.0.0",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.18.0",
    "eslint-config-loopback": "^8.0.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.4",
    "ngrok": "^2.2.6",
    "nsp": "^2.6.3",
    "offline-plugin": "^4.6.2",
    "postcss": "^5.2.11",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^5.1.2",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.8",
    "style-loader": "^0.13.1",
    "stylelint": "^7.10.1",
    "stylelint-config-dev": "^1.4.0",
    "stylelint-order": "^0.4.3",
    "stylelint-scss": "^2.1.0",
    "stylelint-suitcss": "^1.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "ts-jest": "^20.0.10",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
